{
  "kind": "collectionType",
  "collectionName": "bookings",
  "info": {
    "singularName": "booking",
    "pluralName": "bookings",
    "displayName": "booking"
  },
  "options": {
    "draftAndPublish": true,
    "beforeCreate": {
        "handler": "api::booking.booking.beforeCreate"
      }
  },
  "pluginOptions": {},
  "attributes": {
    "booking_date": {
      "type": "datetime",
      "required": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "confirmed",
        "cancelled"
      ]
    },
    "special_requests": {
      "type": "string"
    },
    "service": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::service.service",
      "inversedBy": "bookings"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "bookings"
    },
    "confirmation_code": {
      "type": "string",
      "unique": true
    }
  }
}
