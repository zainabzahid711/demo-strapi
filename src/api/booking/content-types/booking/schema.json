{
  "kind": "collectionType",
  "collectionName": "bookings",
  "info": {
    "singularName": "booking",
    "pluralName": "bookings",
    "displayName": "booking",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "confirmed",
        "cancelled"
      ]
    },
    "confirmation_code": {
      "type": "string",
      "unique": true
    },
    "room": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::room.room"
    },
    "startDate": {
      "type": "date"
    },
    "endDate": {
      "type": "date"
    },
    "guest": {
      "type": "integer"
    },
    "totalPrice": {
      "type": "biginteger"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "user_bookings"
    },
    "customer_name": {
      "type": "string"
    },
    "customer_email": {
      "type": "email"
    }
  }
}
